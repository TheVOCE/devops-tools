{
    "name": "voce-vscode-copilot-extension",
    "publisher": "NOrschel&HarryBin",
    "displayName": "VOCE",
    "description": "VOCE, a 'VscOde Copilot Extension' supporting referencing of GitHub issues and pullrequests",
    "repository": {
        "type": "git",
        "url": "https://github.com/Microsoft/vscode-extension-samples"
    },
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.90.0"
    },
    "categories": [
        "AI",
        "Chat",
        "Other"
    ],
    "activationEvents": [],
    "enabledApiProposals": [
        "chatVariableResolver",
        "defaultChatParticipant",
        "languageModelSystem"
    ],
    "extensionDependencies": [
        "github.copilot-chat"
    ],
    "capabilities": {
        "authentication": {
            "supportsMultipleAccounts": false
        }
    },
    "authentication": [
        {
            "id": "github",
            "scopes": [
                "repo"
            ]
        }
    ],
    "contributes": {
        "chatParticipants": [
            {
                "id": "nohb.voce",
                "fullName": "VOCE - VscOde Copilot Extension",
                "name": "issues",
                "description": "VOCE, a 'VscOde Copilot Extension' supporting referencing of GitHub issues and pullrequests",
                "isSticky": true,
                "commands": [
                    {
                        "name": "issue",
                        "description": "get data from GitHub issue"
                    },
                    {
                        "name": "pullrequest",
                        "description": "get data from GitHub pullrequest"
                    }
                ]
            }
        ],
        "commands": [
            {
                "command": "VOCE.namesInEditor",
                "title": "VOCE - VscOde Copilot Extension"
            }
        ]
    },
    "main": "./out/extension.js",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall_no_longer_required": "npm run setupTiktoken",
        "setupTiktoken": "run-script-os",
        "setupTiktoken:linux:darwin": "cp node_modules/@vscode/prompt-tsx/dist/base/tokenizer/cl100k_base.tiktoken out/",
        "setupTiktoken:windows": "copy node_modules\\@vscode\\prompt-tsx\\dist\\base\\tokenizer\\cl100k_base.tiktoken out\\"
    },
    "devDependencies": {
        "@octokit/rest": "^21.0.1",
        "@types/node": "^20.14.11",
        "@vscode/prompt-tsx": "^0.2.6-alpha",
        "eslint": "^9.7.0",
        "run-script-os": "^1.1.6",
        "simple-git": "^3.25.0",
        "tslint": "^6.1.3",
        "typescript": "^5.5.3"
    }
}